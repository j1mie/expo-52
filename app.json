{
  "expo": {
    "name": "expo-52",
    "slug": "expo-52",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Geist-Black.otf",
            "assets/fonts/Geist-Bold.otf",
            "assets/fonts/Geist-ExtraBold.otf",
            "assets/fonts/Geist-ExtraLight.otf",
            "assets/fonts/Geist-Light.otf",
            "assets/fonts/Geist-Medium.otf",
            "assets/fonts/Geist-Regular.otf",
            "assets/fonts/Geist-SemiBold.otf",
            "assets/fonts/Geist-Thin.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
